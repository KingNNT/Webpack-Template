{
  "name": "webpack",
  "version": "0.0.1",
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "dev": "npm run development",
    "development": "npx webpack --config webpack.development.js",
    "watch": "npx webpack --watch",
    "watch-poll": "npx webpack --watch --watch-options-poll=1000",
    "prod": "npm run production",
    "production": "npx webpack --config webpack.production.js",
    "clean": "rm -rf ./dist",
    "reset": "rm -rf ./dist && rm -rf ./node_modules"
  },
  "author": "KingNNT",
  "license": "ISC",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.1",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.6",
    "dotenv": "^10.0.0",
    "file-loader": "^6.2.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "npm-watch": "^0.9.0",
    "postcss": "^8.3.2",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
